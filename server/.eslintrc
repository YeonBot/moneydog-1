{
  "extends": "eslint-config-google",
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "semi": [
      "error",
      "always"
    ],
    "new-cap": "off",
    "object-curly-spacing": "off",
    "eol-last": "off",
    "max-len": "off",
    "block-spacing": "off",
    "brace-style": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  }
}